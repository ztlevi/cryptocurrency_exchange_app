{
    "env": {
        "node": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "arrowFunctions": true,
            "blockBindings": true,
            "classes": true,
            "defaultParameters": true,
            "destructuring": true,
            "forOf": true,
            "generators": true,
            "modules": true,
            "objectLiteralComputedProperties": true,
            "objectLiteralDuplicateProperties": true,
            "objectLiteralShorthandMethods": true,
            "objectLiteralShorthandProperties": true,
            "regexUFlag": true,
            "regexYFlag": true,
            "restParams": true,
            "spread": true,
            "superInFunctions": true,
            "templateStrings": true,
            "unicodeCodePointEscapes": true,
            "globalReturn": true
        }
    },
    "rules": {
        "consistent-return": [
            0
        ],
        "key-spacing": [
            0
        ],
        "quotes": [
            0
        ],
        "new-cap": [
            0
        ],
        "no-multi-spaces": [
            0
        ],
        "no-shadow": [
            0
        ],
        "no-unused-vars": [
            1
        ],
        "no-use-before-define": [
            2,
            "nofunc"
        ]
    }
}
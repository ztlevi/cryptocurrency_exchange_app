{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon --exec babel-node --presets=env --ignore='node_modules' -- ./src/bin/www",
    "clean": "rm -rf dist",
    "build": "npm run clean && mkdir dist && babel src -s --copy-files -d dist && babel src/bin/www -s -o dist/bin/www",
    "production": "npm run build && node dist/bin/www",
    "start": "node -r babel-register ./src/bin/www"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.15.5",
    "firebase": "^4.10.1",
    "firebase-admin": "^5.9.1",
    "jade": "~1.11.0",
    "morgan": "~1.9.0",
    "serve-favicon": "~2.4.5",
    "thenjs": "^2.0.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "prettier": "^1.11.0"
  }
}
